{
  "name": "@ledesign/terrain",
  "version": "1.0.0",
  "description": "Terrain analysis and surveying module for LeDesign platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./geotiff": {
      "types": "./dist/geotiff-terrain.d.ts",
      "import": "./dist/geotiff-terrain.mjs",
      "require": "./dist/geotiff-terrain.js"
    },
    "./geotiff-terrain": {
      "types": "./dist/geotiff-terrain.d.ts",
      "import": "./dist/geotiff-terrain.mjs",
      "require": "./dist/geotiff-terrain.js"
    },
    "./dem-service": {
      "types": "./dist/dem-service.d.ts",
      "import": "./dist/dem-service.mjs",
      "require": "./dist/dem-service.js"
    },
    "./terrain-service": {
      "types": "./dist/terrain-service.d.ts",
      "import": "./dist/terrain-service.mjs",
      "require": "./dist/terrain-service.js"
    },
    "./dwg": {
      "types": "./dist/dwg/index.d.ts",
      "import": "./dist/dwg/index.mjs",
      "require": "./dist/dwg/index.js"
    },
    "./geo": {
      "types": "./dist/geo/index.d.ts",
      "import": "./dist/geo/index.mjs",
      "require": "./dist/geo/index.js"
    },
    "./triangulation": {
      "types": "./dist/triangulation/index.d.ts",
      "import": "./dist/triangulation/index.mjs",
      "require": "./dist/triangulation/index.js"
    },
    "./volume-calculation": {
      "types": "./dist/volume-calculation.d.ts",
      "import": "./dist/volume-calculation.mjs",
      "require": "./dist/volume-calculation.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@mlightcad/libredwg-web": "^0.6.3",
    "delaunator": "^5.0.1",
    "geotiff": "^2.1.4-beta.1",
    "ml-matrix": "^6.12.1",
    "papaparse": "^5.5.3",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@types/delaunator": "^5.0.3",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^22.10.5",
    "@types/papaparse": "^5.5.2",
    "@types/three": "^0.182.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
